<!-- här ska finnas slideshow -->
<!-- Container -->
<link rel="stylesheet" href="~/css/slideshow.css"/>



<div class="containerSS">

    <!-- Full-width images with number text -->
    <div class="mySlides">
        <div class="numbertext">1 / 4</div>
        <img src="/Pictures/Slideshow/Evolved-C418.png" style="width:100%">
    </div>

    <div class="mySlides">
        <div class="numbertext">2 / 4</div>
        <img src="/Pictures/Slideshow/lodsa.png" style="width:100%">
    </div>

    <div class="mySlides">
        <div class="numbertext">3 / 4</div>
        <img src="/Pictures/Slideshow/lenapop.png" style="width:100%">
    </div>

    <div class="mySlides">
        <div class="numbertext">4 / 4</div>
        <img src="/Pictures/Slideshow/lolman.png" style="width:100%">
    </div>

    <!-- Next and previous buttons -->
    <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
    <a class="next" onclick="plusSlides(1)">&#10095;</a>

    <!-- Image text -->
    <div class="caption-container">
        <p id="caption"></p>
    </div>

    <!-- Thumbnail images -->
    <div class="rowSS">
        <div class="columnSS">
            <img class="demo cursor" src="/Pictures/Slideshow/Evolved-C418.png" style="width:100%" onclick="currentSlide(1)" alt="Angry">
        </div>
        <div class="columnSS">
            <img class="demo cursor" src="/Pictures/Slideshow/lodsa.png" style="width:100%" onclick="currentSlide(2)" alt="Bird">
        </div>
        <div class="columnSS">
            <img class="demo cursor" src="/Pictures/Slideshow/lenapop.png" style="width:100%" onclick="currentSlide(3)" alt="Egg">
        </div>
        <div class="columnSS">
            <img class="demo cursor" src="/Pictures/Slideshow/lolman.png" style="width:100%" onclick="currentSlide(4)" alt="Speed">
        </div>
    </div>
</div>

<script src="~/js/slideshow.js"></script>